<% if @user != nil %>
	<h1><%= @user.name%></h1>
	<p>(Other userprofile information to be available here)</p>
	<div class="listings">
		<h2>All Listings for <%= @user.name%></h2>
		<table class="four">
		  <tr class="four">
		  	<% @listings.each do |l| %>
				  <td><div class="tdfour">
				  	<%= link_to  :controller => 'listings', :action => 'listing', :id => l.id do %>
					  	<%= image_tag("", :alt =>"picture", :class =>"four imagebtn") %>
					  	<h3><%= l.name %></h3>
				  	<% end %>
				  <%= l.description %></div></td>
				<% end %>
					<td><div class="tdfour"></div></td>
		  </tr>
		</table>
	</div>
<% else %>
	<h1> asdfasdf</h1>
<%end%>